   "id": "d44aa191",
   "metadata": {},
   "outputs": [],
   "source": [
    "def solve_linear(equation,var='x'):\n",
    "    expression = equation.replace(\"=\",\"-(\")+\")\"\n",
    "    grouped = eval(expression.replace(var,'1j'))\n",
    "    return -grouped.real/grouped.imag\n",
    "    "
   ]
  },
